function AdClicked(url)
{ 
    var clickLineDisabled = "$dcli";
    if(clickLineDisabled=="1")
    {
        return;
    }
    
    var winOpen = "1";
    if(winOpen == "1")
    {
        window.open(url,"");
    }else
    {
        parent.location.href=url; 
    }
}
